                            	    	return false;
                            	    }
                             		content = $("#problem_").val();
                             		if( content == ''){
                            	    	alert('内容不能为空');
                            	    	return false;
                            	    }
                             		token = $("#token_").val();
                            		$.post(
                             		'/guestbook.php?rec=ajax_insert',{
                             			name:name,
                             			email: email,
                             			tel: tel,
                             			captcha: captcha,
                             			content: $("#problem_").val(),
                             			token : token
                             			},function(data){
                             			alert(data);
                             		},'json')
                            		return false;
                            	})
                            })
                             
                             {/literal}
